{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.28",
    "yarn": "^1.22.10",
    "yup": "^0.29.3"
  },
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "dbinit": "ts-node ./node_modules/typeorm/cli.js migration:generate -n CreateDatabase",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^8.0.29",
    "@types/yup": "^0.29.9",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "3.3.3333"
  }
}
